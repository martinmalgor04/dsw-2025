  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".real.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage-real",
  "moduleNameMapper": {
    "^@logistics/(.*)$": "<rootDir>/../../shared/$1/src"
  },
  "testTimeout": 30000
}
